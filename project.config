[project]
	description = STARAYS
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Ray (starays)
[access "refs/for/refs/*"]
	read = group user/Ray (starays)
	push = group user/Ray (starays)
	label-Code-Review = -2..+2 group user/Ray (starays)
	label-Verified = -1..+1 group user/Ray (starays)
	rebase = group user/Ray (starays)
	removeReviewer = group user/Ray (starays)
	submit = group user/Ray (starays)
[access "refs/heads/*"]
	read = group user/Ray (starays)
	create = group user/Ray (starays)
	push = +force group user/Ray (starays)
	pushMerge = group user/Ray (starays)
[access "refs/tags/*"]
	push = +force group user/Ray (starays)
	pushTag = group user/Ray (starays)
	pushSignedTag = group user/Ray (starays)
